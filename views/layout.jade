//- layout.jade
doctype html
html(lang="en")
  head
    block title
      title Yoda the Travel Agent

    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1")

    link(rel="shortcut icon", href="/images/favicon.ico", type="image/x-icon")
    link(rel="icon", href="/images/favicon.ico", type="image/x-icon")
    link(rel="stylesheet", href="/css/watson-bootstrap-dark.css")
    link(rel="stylesheet", href="/css/browser-compatibility.css")
    link(rel="stylesheet", href="/css/style.css")
    block extrahead
  body
    div.container
      div.header.text-center
        div
          img(src="/images/yoda-small.jpg")
        div
          h2 Yoda the Travel Agent
      block row-content
    script(type='text/javascript', src="/js/css_browser_selector.js")
    script(type='text/javascript', src="/js/jquery-1.11.1.min.js")
    script(type='text/javascript', src="/js/example_input.js")